{
    "name": "abtestify",
    "version": "0.0.1",
    "type": "module",
    "description": "abtestify is a package to do ab testing in your project in next.js react or any javascript frameworks",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": ["dist"],
    "repository": "https://github.com/pranav2012/abtestify.git",
    "author": "pranav2012 <p2012agarwal@gmail.com>",
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
        "build": "tsup",
        "start": "node dist/index.js",
        "prepare": "husky",
        "lint": "eslint \"**/*.{js,ts}\"",
        "format": "prettier --check .",
        "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
        "format:fix": "prettier --write .",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "abtesting",
        "abtestify",
        "abtestify-react",
        "abtestify-nextjs",
        "abtestify-nodejs",
        "abtestify-express",
        "ab-test",
        "ab-test-react",
        "ab-experiment",
        "ab-experiment-react",
        "ab-experiment-nextjs",
        "experiment",
        "abtest",
        "abtestify-node"
    ],
    "devDependencies": {
        "@commitlint/config-conventional": "^19.1.0",
        "@types/jest": "^29.5.12",
        "commitlint": "^19.2.1",
        "eslint": "^9.0.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.4.4",
        "typescript-eslint": "^7.5.0"
    }
}
